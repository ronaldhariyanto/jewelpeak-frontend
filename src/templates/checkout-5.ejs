<!DOCTYPE html>
<html lang="en">
  <head>
    <% include partials/head %>
  </head>
  <body>
    <% include partials/header %>
    <% include partials/navigation %>
    <% include partials/breadcrumb %>


    <section class="checkout">
      <div class="container">
        <div class="row">
          <div class="col-lg-9 col-md-9 col-sm-7 col-xs-6 col-xxs-12 text-center-xs">
            <h1 class="checkout-title"><i class="glyphicon glyphicon-shopping-cart"></i> <span>Checkout</span></h1>
          </div>
          <div class="col-lg-3 col-md-3 col-sm-5 rightSidebar col-xs-6 col-xxs-12 text-center-xs">
            <h4 class="checkout-back-to"><a href=""><i class="fa fa-chevron-left"></i> Back to shopping </a></h4>
          </div>
        </div>

        <div class="row">
          <div class="col-lg-9 col-md-9 col-sm-12">
            <div class="row user-info">
              <div class="col-xs-12 col-sm-12">
                <ul class="orderstep">
                  <li>
                    <a href="checkout-1.html"> <i class="fa fa-map-marker"></i> <span> address</span></a>
                  </li>
                  <li>
                    <a href="checkout-2.html"> <i class="fa fa fa-envelope"></i><span> Billing </span></a>
                  </li>
                  <li>
                    <a href="checkout-3.html"><i class="fa fa-truck"> </i><span>Shipping</span> </a>
                  </li>
                  <li>
                    <a href="checkout-4.html"><i class="fa fa-money"> </i><span>Payment</span> </a>
                  </li>
                  <li class="active">
                    <a href="checkout-5.html"><i class="fa fa-check-square"> </i><span>Order</span></a>
                  </li>
                </ul>
              </div>
            </div>

            <div class="row user-info">
              <div class="col-lg-12">
                <h2>Review Order</h2>
              </div>
              <div class="col-xs-12 col-sm-12">
                <div class="cart cart-content checkout-review">
                  <table class="cart-table table">
                    <tbody>
                      <tr class="cart-product cart-table-header">
                        <th style="width:15%">Product</th>
                        <th class="checkoutReviewTdDetails">Details</th>
                        <th style="width:10%">Unit Price</th>
                        <th class="hidden-xs" style="width:5%">QNT</th>
                        <th class="hidden-xs" style="width:10%">Discount</th>
                        <th style="width:15%">Total</th>
                      </tr>
                      <tr class="cart-product">
                        <td class="cart-productThumb">
                          <div>
                            <a href="product-details.html"><img width="86" src="assets/images/product/product-600-03.jpg"></a>
                          </div>
                        </td>
                        <td>
                          <div class="cart-description">
                            <h4><a href="product-details.html">Diamond</a></h4>
                            <span class="size">2 gr</span>
                          </div>
                        </td>
                        <td class="delete">
                          <div class="price">
                            S$1,016.00
                          </div>
                        </td>
                        <td class="hidden-xs">1</td>
                        <td class="hidden-xs">0</td>
                        <td class="price">S$1,016.00</td>
                      </tr>
                    </tbody>
                  </table>
                </div>
                <div class="cart cost-details">
                  <div class="table-block" id="order-detail-content">
                    <table class="std table" id="cart-summary">
                      <tr>
                        <td>Total products</td>
                        <td class="price">S$1,016.00</td>
                      </tr>
                      <tr style="">
                        <td>Shipping</td>
                        <td class="price"><span class="success">Free shipping!</span></td>
                      </tr>
                      <tr class="cart-total-price">
                        <td>Total (tax excl.)</td>
                        <td class="price">S$1,016.00</td>
                      </tr>
                      <tr>
                        <td>Total tax</td>
                        <td class="price" id="total-tax">0.00</td>
                      </tr>
                      <tr>
                        <td>Total</td>
                        <td class="price" id="total-price">S$1,016.00</td>
                      </tr>
                      <tbody></tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>

            <div class="cart-footer">
              <div class="box-footer">
                <div class="pull-left">
                  <a class="btn btn-default" href="checkout-4.html">
                    <i class="fa fa-arrow-left"></i> &nbsp; Payment method
                  </a>
                </div>
                <div class="pull-right">
                  <a class="btn btn-primary btn-small " href="checkout-3.html">
                    Confirm Order &nbsp; <i class="fa fa-check"></i>
                  </a>
                </div>
              </div>
            </div>
          </div>

          <div class="col-lg-3 col-md-3 col-sm-5 sidebar-right">
            <div class="content-box">
              <div class="cart cost-details">
                  <div class="table-block" id="order-detail-content">
                    <a class="btn btn-primary btn-lg btn-block " title="checkout" href="">
                      Proceed to checkout &nbsp; <i class="fa fa-arrow-right"></i>
                    </a>

                    <div class="cart-mini-table">
                      <table id="cart-summary" class="table">
                        <tbody>
                          <tr>
                            <td>Total products</td>
                            <td class=""><b>S$1,200.00</b></td>
                          </tr>
                          <tr style="">
                            <td>Shipping</td>
                            <td class="price"><span class="success">Free shipping!</span></td>
                          </tr>
                          <tr class="cart-total-price ">
                            <td>Total (tax excl.)</td>
                            <td class=""><b>S$1,200.00</b></td>
                          </tr>
                          <tr>
                            <td>Total tax</td>
                            <td class="" id="total-tax"><b>S$0.00</b></td>
                          </tr>
                          <tr>
                            <td> Total</td>
                            <td class="price" id="total-price">S$1,200.00</td>
                          </tr>
                        </tbody>
                      </table>
                    </div>
                  </div>
              </div>
            </div>
          </div>

    </div>
      </div>
    </section>

    <% include partials/footer %>
    <% include partials/scripts %>
  </body>
</html>